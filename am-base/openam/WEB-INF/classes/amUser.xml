<!DOCTYPE ServicesConfiguration
        PUBLIC "=//iPlanet//Service Management Services (SMS) 1.0 DTD//EN"
        "jar://com/sun/identity/sm/sms.dtd">

<ServicesConfiguration>
    <Service name="iPlanetAMUserService" version="1.0">
        <Schema
                serviceHierarchy="/DSAMEConfig/iPlanetAMUserService"
                i18nFileName="amUser"
                i18nKey="iplanet-am-user-service-description"
                resourceName="user"
                revisionNumber="50">
            <Global>
                <AttributeSchema name="serviceObjectClasses"
                                 type="list"
                                 syntax="string"
                                 i18nKey="">
                    <DefaultValues>
                        <Value>iplanet-am-user-service</Value>
                    </DefaultValues>
                </AttributeSchema>
            </Global>

            <Dynamic>
                <AttributeSchema name="preferredlanguage"
                                 type="single"
                                 syntax="string"
                                 any="display"
                                 i18nKey="">
                </AttributeSchema>
                <AttributeSchema name="preferredtimezone"
                                 type="single"
                                 syntax="string"
                                 any="display"
                                 resourceName="preferredTimezone"
                                 i18nKey="d102"
                                 order="100">
                </AttributeSchema>
                <AttributeSchema name="preferredlocale"
                                 type="single_choice"
                                 syntax="string"
                                 any="display"
                                 i18nKey="">
                    <ChoiceValues>
                        <ChoiceValuesClassName
                                className="com.sun.identity.common.admin.PossibleLocales"/>
                    </ChoiceValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-admin-start-dn"
                                 type="single"
                                 syntax="dn"
                                 any="display"
                                 i18nKey="d104"
                                 order="200"
                                 resourceName="adminDNStartingView">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-login-status"
                                 type="single_choice"
                                 syntax="string"
                                 any="display"
                                 i18nKey="d105"
                                 order="300"
                                 resourceName="defaultUserStatus">
                    <ChoiceValues>
                        <ChoiceValue i18nKey="u200">Active</ChoiceValue>
                        <ChoiceValue i18nKey="u201">Inactive</ChoiceValue>
                    </ChoiceValues>
                    <DefaultValues>
                        <Value>Active</Value>
                    </DefaultValues>
                </AttributeSchema>
            </Dynamic>

            <User>
                <AttributeSchema name="uid"
                                 type="single"
                                 syntax="string"
                                 any="required|filter"
                                 validator="RequiredValueValidator"
                                 i18nKey=""
                                 order="400">
                </AttributeSchema>
                <AttributeSchema name="givenName"
                                 type="single"
                                 syntax="string"
                                 any="optional|filter|display"
                                 i18nKey="u102"
                                 order="500">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="sn"
                                 type="single"
                                 syntax="string"
                                 any="required|filter|display"
                                 i18nKey="u103"
                                 order="600">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="cn"
                                 type="single"
                                 any="required|filter|display"
                                 syntax="string"
                                 i18nKey="u104"
                                 order="700">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="userPassword"
                                 type="single"
                                 syntax="password"
                                 any="required"
                                 validator="RequiredValueValidator"
                                 i18nKey="u105"
                                 order="800">
                </AttributeSchema>
                <AttributeSchema name="mail"
                                 type="single"
                                 syntax="email"
                                 any="display"
                                 i18nKey="u106"
                                 order="1000">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="employeeNumber"
                                 type="single"
                                 syntax="number"
                                 any="adminDisplay"
                                 i18nKey="u107"
                                 order="1100">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="telephoneNumber"
                                 type="single"
                                 syntax="string"
                                 any="display"
                                 i18nKey="u108"
                                 order="1200">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="postalAddress"
                                 type="single"
                                 syntax="string"
                                 any="display"
                                 i18nKey="u110"
                                 order="1400">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="inetUserStatus"
                                 type="single_choice"
                                 syntax="string"
                                 any="required|filter|adminDisplay"
                                 i18nKey="u111"
                                 order="1500">
                    <IsOptional/>
                    <ChoiceValues>
                        <ChoiceValue i18nKey="u200">Active</ChoiceValue>
                        <ChoiceValue i18nKey="u201">Inactive</ChoiceValue>
                    </ChoiceValues>
                    <DefaultValues>
                        <Value>Active</Value>
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-auth-config"
                                 type="single_choice"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey="u113"
                                 order="1700">
                    <IsOptional/>
                    <ChoiceValues>
                        <ChoiceValuesClassName
                                className="com.sun.identity.authentication.service.ConfiguredAuthServices"/>
                    </ChoiceValues>
                    <DefaultValues>
                        <Value>[Empty]</Value>
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-alias-list"
                                 type="list"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey=""
                                 order="1800">
                    <IsOptional/>
                    <DefaultValues>
                        <Value/>
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-success-url"
                                 type="list"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey="u116"
                                 order="1900">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-user-failure-url"
                                 type="list"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey="u117"
                                 order="2000">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="sunIdentityMSISDNNumber"
                                 type="list"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey="u122"
                                 order="2200">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="lastEmailSent"
                                 type="single"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey=""
                                 order="2300">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="retryLimitNodeCount"
                                 type="list"
                                 syntax="string"
                                 any="adminDisplay"
                                 i18nKey=""
                                 order="2300">
                    <IsOptional/>
                </AttributeSchema>
                <AttributeSchema name="RequiredValueValidator"
                                 type="validator"
                                 syntax="string">
                    <DefaultValues>
                        <Value>com.sun.identity.sm.RequiredValueValidator</Value>
                    </DefaultValues>
                </AttributeSchema>
            </User>
        </Schema>
    </Service>
</ServicesConfiguration>

