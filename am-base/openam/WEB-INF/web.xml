<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>OpenAM Services</display-name>

    <listener>
        <listener-class>com.sun.identity.setup.SystemStartupInjectorListener</listener-class>
    </listener>

    <listener>
        <listener-class>org.forgerock.openam.core.guice.OpenAMServletContextListener</listener-class>
    </listener>

    <listener>
        <listener-class>org.forgerock.guice.core.GuiceInitialisationFilter</listener-class>
    </listener>

    <listener>
        <listener-class>org.forgerock.openam.identity.idm.AMIdentityRepositoryListenerInitializer</listener-class>
    </listener>

    <filter>
        <filter-name>RequestEntitySizeVerificationFilter</filter-name>
        <filter-class>org.forgerock.openam.validation.RequestEntitySizeVerificationFilter</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter>
        <filter-name>amSetupFilter</filter-name>
        <filter-class>com.sun.identity.setup.AMSetupFilter</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter>
        <filter-name>ResponseValidationFilter</filter-name>
        <filter-class>org.forgerock.openam.validation.ResponseValidationFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>blacklisted-headers</param-name>
            <param-value>Server</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>SecureCookieFilter</filter-name>
        <filter-class>org.forgerock.openam.headers.SecureCookieFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <!-- Add any cookies that should be excluded from upgrade to secure cookies here -->
            <param-name>excludes</param-name>
            <param-value></param-value>
        </init-param>
    </filter>
    <!--
    To override the default User-Agent exclusion patterns for SameSite=none cookies, uncomment
    the following filter definition and update the excluded patterns, one pattern per line -->
    <!--
    <filter>
        <filter-name>DisableSameSiteCookiesFilter</filter-name>
        <filter-class>org.forgerock.openam.headers.DisableSameSiteCookiesFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>excludedUserAgentPatterns</param-name>
            <param-value>
                (?!Chrom(e|ium))Version/(1[0-2]|[1-9])(\.\d+)* .*Safari/
                \(iP.+; CPU .*OS (1[0-2]|[1-9])_[_\d]*.*\) AppleWebKit/
                ^Mozilla/[\.\d]+ \(Macintosh;.*Mac OS X 10_14[_\d]*\) AppleWebKit/[\.\d]+ \(KHTML, like Gecko\)$
                Chrom(e|ium)[^ /]*/(5[1-9]|6[0-6])\.[\.\d]*
                UCBrowser/(12\.13\.[01][^\d]|12\.1[0-2]\.|12\.[0-9]\.|1[01]\.|[0-9]\.)[\.\d]*
            </param-value>
        </init-param>
    </filter>
    -->
    <filter>
        <filter-name>FQDNValidationFilter</filter-name>
        <filter-class>org.forgerock.openam.validation.FQDNValidationFilter</filter-class>
    </filter>
    <filter>
        <filter-name>XUIFilter</filter-name>
        <filter-class>org.forgerock.openam.xui.XUIFilter</filter-class>
    </filter>
    <filter>
        <filter-name>SecurityHeaders</filter-name>
        <filter-class>org.forgerock.openam.headers.SetHeadersFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>X-Frame-Options</param-name>
            <param-value>SAMEORIGIN</param-value>
        </init-param>
        <init-param>
            <param-name>excludes</param-name>
            <param-value>/connect/checkSession</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>NoSniffFilter</filter-name>
        <filter-class>org.forgerock.openam.headers.SetHeadersFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>X-Content-Type-Options</param-name>
            <param-value>nosniff</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>CachePrivate</filter-name>
        <filter-class>org.forgerock.openam.headers.SetHeadersFilter</filter-class>
        <init-param>
            <param-name>Cache-Control</param-name>
            <param-value>private</param-value>
        </init-param>
        <init-param>
            <param-name>excludes</param-name>
            <param-value>/serverinfo/*,/serverinfo/version,/serverinfo/cookieDomains</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>CacheForFiveMinutes</filter-name>
        <filter-class>org.forgerock.openam.headers.SetHeadersFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>Cache-Control</param-name>
            <param-value>public, max-age=300</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>CacheForAMonth</filter-name>
        <filter-class>org.forgerock.openam.headers.SetHeadersFilter</filter-class>
        <async-supported>true</async-supported>
        <init-param>
            <param-name>Cache-Control</param-name>
            <param-value>public, max-age=2592000</param-value>
        </init-param>
        <init-param>
            <param-name>excludes</param-name>
            <param-value>/XUI/,/XUI/index.html,/ui-admin/,/ui-admin/index.html</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>AuditContextFilter</filter-name>
        <filter-class>org.forgerock.openam.audit.context.AuditContextFilter</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter>
        <filter-name>NotificationsWebSocketFilter</filter-name>
        <filter-class>org.forgerock.openam.notifications.NotificationsWebSocketFilter</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter>
        <filter-name>DefaultProtocolVersionFilter</filter-name>
        <filter-class>org.forgerock.openam.rest.ProtocolVersionFilter</filter-class>
        <async-supported>true</async-supported>
    </filter>
    <filter>
        <filter-name>DataStoreConsistencyFilter</filter-name>
        <filter-class>org.forgerock.openam.services.datastore.DataStoreConsistencyFilter</filter-class>
    </filter>

    <!-- filter declaration -->

    <!--
    AuditContextFilter must be listed first so that it is the first filter to be evaluated.
    This ensures that the audit request context is initialised before any audit events or debug logs are emitted.
    -->
    <filter-mapping>
        <filter-name>AuditContextFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <!-- Access audit filter for Debug.jsp and ssoadm.jsp pages -->
    <filter>
        <filter-name>DebugAuditFilter</filter-name>
        <filter-class>org.forgerock.openam.audit.servlet.AuditAccessServletFilter</filter-class>
        <init-param>
            <param-name>auditing-component</param-name>
            <param-value>JATO</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>DebugAuditFilter</filter-name>
        <url-pattern>/Debug.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>DebugAuditFilter</filter-name>
        <url-pattern>/Logback.jsp</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>SsoAdmJspAuditFilter</filter-name>
        <filter-class>org.forgerock.openam.audit.servlet.AuditAccessServletFilter</filter-class>
        <init-param>
            <param-name>auditing-component</param-name>
            <param-value>SSOADM</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>SsoAdmJspAuditFilter</filter-name>
        <url-pattern>/ssoadm.jsp</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>RequestEntitySizeVerificationFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>amSetupFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FQDNValidationFilter</filter-name>
        <url-pattern>/XUI/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FQDNValidationFilter</filter-name>
        <url-pattern>/ui-admin/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FQDNValidationFilter</filter-name>
        <url-pattern>/oauth2/device/user</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>FQDNValidationFilter</filter-name>
        <url-pattern>/oauth2/authorize</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>XUIFilter</filter-name>
        <url-pattern>/UI/*</url-pattern>
        <dispatcher>FORWARD</dispatcher>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>XUIFilter</filter-name>
        <url-pattern>/idm/EndUser</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SecurityHeaders</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>NoSniffFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CachePrivate</filter-name>
        <url-pattern>/json/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>ResponseValidationFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <!-- The DisableSameSiteCookiesFilter should always come before the SecureCookieFilter -->
        <filter-name>DisableSameSiteCookiesFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SecureCookieFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CacheForFiveMinutes</filter-name>
        <url-pattern>/XUI/index.html</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CacheForAMonth</filter-name>
        <url-pattern>/js/*</url-pattern>
        <url-pattern>/ui-admin/*</url-pattern>
        <url-pattern>/XUI/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CacheForFiveMinutes</filter-name>
        <url-pattern>/ui-admin/index.html</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>NotificationsWebSocketFilter</filter-name>
        <url-pattern>/notifications</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>DefaultProtocolVersionFilter</filter-name>
        <url-pattern>/json/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>DataStoreConsistencyFilter</filter-name>
        <url-pattern>/json/*</url-pattern>
        <url-pattern>/federation/*</url-pattern>
    </filter-mapping>

